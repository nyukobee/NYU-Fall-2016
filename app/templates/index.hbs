<h1>Home Page</h1>
<div class="row">
{{#each model as |post|}}
  <div class="col-lg-3 col-sm-6 col-xs-12">
    <div class="thumbnail">
      {{ember-cli-holderjs src_url="holder.js/100px200" class="img-responsive"}}
      <div class="caption">
        <h4>{{post.title}} <small>{{moment-from-now post.timestamp}}</small></h4>
        <p>{{post.body}}</p>
      </div>
    </div>
  </div>
{{/each}}
</div>